{
    "name": "theomessin/laravel-stalker",
    "description": "A Laravel package to help you stalk your users.",
    "keywords": [
        "theomessin",
        "laravel-stalker"
    ],
    "homepage": "https://github.com/theomessin/laravel-stalker",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Theodore Messinezis",
            "email": "theodore@messinezis.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/contracts": "^6.9|^7.0",
        "illuminate/database": "^6.9|^7.0",
        "illuminate/support": "^6.9|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0",
        "slevomat/coding-standard": "~5.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Theomessin\\Stalker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theomessin\\Stalker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Theomessin\\Stalker\\StalkerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
